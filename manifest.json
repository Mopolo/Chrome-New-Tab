{
  "manifest_version": 2,

  "name": "New Tab",
  "description": "Simple new tab",
  "version": "1.0",
  "default_locale": "en",
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  },

  "permissions": [
    "storage",
    "tabs"
  ],

  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
